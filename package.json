  "keywords": [
    "Time tracker"
  ],
  "pre-commit": [
    "lint"
  ],
  "homepage": "https://github.com/HsuTing/cat-time-tracker/",
  "repository": {
    "type": "git",
    "url": "get+https://github.com/HsuTing/cat-time-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/HsuTing/cat-time-tracker/issues/"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
