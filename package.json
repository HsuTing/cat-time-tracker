{
  "engines": {
    "node": "8.1.2",
    "yarn": "0.24.6",
    "npm": "5.3.0"
  },
  "scripts": {
    "webpack-server": "webpack-dev-server --content-base src --hot --inline",
    "webpack": "cross-env NODE_ENV=production webpack",
    "lint": "eslint --cache ./src ./bin --ext .js",
    "lint:watch": "esw --cache ./src ./bin --ext .js -w --color",
    "babel": "rm -rf ./lib && babel src --out-dir lib",
    "babel:watch": "rm -rf ./lib && babel -w src --out-dir lib",
    "prepublish": "yarn babel",
    "test-server": "cross-env NODE_ENV=test nodemon ./lib/index.js start",
    "graphql": "babel src --out-dir lib --ignore components,containers,mutations,public,routers && build-graphql --schema ./lib/server/schemas/schema.js",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql",
    "relay:watch": "relay-compiler --src ./src --schema ./schema.graphql --watch",
    "build": "yarn graphql && yarn relay && yarn babel",
    "prod": "yarn graphql && yarn relay && yarn babel && yarn webpack",
    "watch": "concurrently -c green \"yarn lint:watch\" \"yarn babel:watch\" \"yarn webpack-server\" \"yarn relay:watch\""
  },
  "bin": {
    "time-tracker": "./lib/index.js"
  },
  "name": "cat-time-tracker",
  "version": "0.2.12",
  "description": "Time tracker",
  "author": {
    "name": "HsuTing",
    "email": "hsuting0106@gmail.com",
    "url": "http://hsuting.com"
  },
  "main": "./lib/index.js",
  "keywords": [
    "Time tracker"
  ],
  "pre-commit": [
    "lint"
  ],
  "homepage": "https://github.com/HsuTing/cat-time-tracker/",
  "repository": {
    "type": "git",
    "url": "get+https://github.com/HsuTing/cat-time-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/HsuTing/cat-time-tracker/issues/"
  },
  "license": "MIT",
